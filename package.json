{
  "name": "cinemarket-contracts",
  "version": "0.0.1",
  "description": "A set of smart contracts for COALA IP and Cinemarket",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "watch": {
    "test": {
      "patterns": [
        "contracts",
        "migrations",
        "test"
      ],
      "extensions": "js,json,sol"
    }
  },
  "dependencies": {
    "ethereumjs-tx": "^1.3.7",
    "ipfs-http-client": "^29.0.1",
    "openzeppelin-solidity": "1.12.0",
    "truffle-hdwallet-provider": "^1.0.2"
  },
  "devDependencies": {
    "npm-watch": "^0.5.0",
    "truffle-test-utils": "^0.1.0",
    "dotenv": "^6.1.0",
    "ethereumjs-wallet": "^0.6.2",
    "truffle-assertions": "^0.6.3",
    "web3": "1.0.0-beta.34"
  },
  "scripts": {
    "pretest": "truffle compile --reset && truffle migrate --reset",
    "migrate": "truffle migrate",
    "test": "truffle test",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CinemarketIO/contracts.git"
  },
  "keywords": [
    "smart",
    "contracts",
    "coala",
    "ip",
    "cinemarket",
    "right"
  ],
  "author": "Cinemarket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CinemarketIO/contracts/issues"
  },
  "homepage": "https://github.com/CinemarketIO/contracts#readme"
}
